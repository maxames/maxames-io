{
  "startOnLoad": false,
  "securityLevel": "loose",
  "fontFamily": "Inter, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
  "fontSize": 14,
  "theme": "base",
  "layout": "elk",
  "themeVariables": {
    "primaryColor": "#ffffff",
    "primaryTextColor": "#3d464d",
    "primaryBorderColor": "#007ee5",
    "lineColor": "#3d464d",
    "background": "#ffffff",
    "mainBkg": "#ffffff",
    "secondBkg": "#f8f9fa",
    "tertiaryBkg": "#e5e7ea",
    "textColor": "#3d464d",
    "darkTextColor": "#3d464d",
    "nodeBorder": "#007ee5",
    "clusterBkg": "#ffffff",
    "clusterBorder": "#007ee5",
    "edgeLabelBackground": "#ffffff",
    "edgeLabelColor": "#3d464d",
    "nodeTextColor": "#3d464d",
    "gridColor": "#e5e7ea",
    "sectionBkgColor": "#ffffff",
    "altSectionBkgColor": "#f8f9fa",
    "fillType0": "#ffffff",
    "fillType1": "#f8f9fa",
    "fillType2": "#e5e7ea",
    "fillType3": "#007ee5",
    "fillType4": "#7b8994",
    "fillType5": "#1a8ceb",
    "fillType6": "#3399f0",
    "fillType7": "#ffffff"
  },
  "flowchart": {
    "rankSpacing": 60,
    "nodeSpacing": 50,
    "padding": 12,
    "useMaxWidth": true,
    "htmlLabels": true
  },
  "elk": {
    "algorithm": "layered",
    "elk.direction": "RIGHT",
    "elk.spacing.nodeNode": "50",
    "elk.layered.spacing.nodeNodeBetweenLayers": "60"
  }
}
